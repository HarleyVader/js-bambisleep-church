{
    "mcpServers": {
        "bambisleep-church": {
            "command": "node",
            "args": [
                "src/mcp/McpServer.js"
            ],
            "cwd": ".",
            "env": {
                "NODE_ENV": "production"
            }
        },
        "bambisleep-church-remote": {
            "url": "https://at.bambisleep.church:9999/mcp",
            "headers": {
                "Content-Type": "application/json",
                "User-Agent": "LMStudio-MCP-Client/1.0"
            }
        },
        "huggingface-mcp": {
            "url": "https://huggingface.co/mcp",
            "headers": {
                "Authorization": "Bearer hf_placeholder_token_replace_with_real_token"
            }
        }
    }
}
