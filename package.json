{
  "name": "js-bambisleep-church",
  "version": "1.0.0",
  "description": "A real-time community-voted link list for different bambisleep themes and categories.",
  "main": "src/app.js",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/HarleyVader/js-bambisleep-church.git"
  },
  "bugs": {
    "url": "https://github.com/HarleyVader/js-bambisleep-church/issues"
  },
  "homepage": "https://github.com/HarleyVader/js-bambisleep-church#readme",
  "scripts": {
    "start": "node scripts/main.js start",
    "dev": "node scripts/main.js start && cd frontend && npm run dev",
    "build": "node scripts/main.js install && node scripts/main.js config && node scripts/main.js build",
    "build:frontend": "cd frontend && npm install && npm run build",
    "build:full": "npm run build:frontend && npm run build",
    "setup": "node scripts/main.js install",
