    "express": "^5.1.0",
    "geoip-lite": "^1.4.10",
    "mongodb": "^6.20.0",
    "node-fetch": "^3.3.2",
    "socket.io": "^4.8.1",
    "zod": "^3.25.76"
  },
  "keywords": [
