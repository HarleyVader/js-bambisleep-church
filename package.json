{
  "name": "js-bambisleep-church",
  "version": "1.0.0",
  "description": "A real-time community-voted link list for different bambisleep themes and categories.",
  "main": "src/app.js",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/HarleyVader/js-bambisleep-church.git"
  },
  "bugs": {
    "url": "https://github.com/HarleyVader/js-bambisleep-church/issues"
  },
  "homepage": "https://github.com/HarleyVader/js-bambisleep-church#readme",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "frontend:dev": "cd frontend && npm run dev",
    "frontend:build": "cd frontend && npm run build",
    "frontend:preview": "cd frontend && npm run preview",
    "build": "npm run frontend:build",
    "dev:full": "concurrently \"npm run dev\" \"npm run frontend:dev\"",
    "test": "node tests/test.cjs",
    "test:full": "node tests/unified-test-suite.cjs",
    "config": "node scripts/generate-config.js",
    "config:readme": "node scripts/generate-readme.js"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.20.0",
    "axios": "^1.12.2",
    "cheerio": "^1.1.2",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "geoip-lite": "^1.4.10",
    "mongodb": "^6.20.0",
    "node-fetch": "^3.3.2",
    "socket.io": "^4.8.1",
    "zod": "^3.25.76"
  },
  "keywords": [
    "bambisleep",
    "community",
    "voting",
    "links",
    "church"
  ],
  "author": "melkanea",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "concurrently": "^9.1.2",
    "jest": "^30.0.0",
    "nodemon": "^3.0.2",
    "socket.io-client": "^4.8.1",
    "supertest": "^7.1.1"
  }
